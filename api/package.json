{
  "name": "api",
  "version": "1.0.0",
  "description": "graphql express api",
  "main": "index.js",
  "scripts": {
    "build": "rimraf dist && babel src -d dist",
    "serve": "node dist/index.js",
    "start": "NODE_ENV=development nodemon --exec babel-node src/index.js",
    "start:prod": "NODE_ENV=production nodemon --exec babel-node src/index.js",
    "test-server": "TEST_DATABASE=mytestdatabase yarn start",
    "test": "mocha",
    "format": "prettier --write \"**/*.js\""
  },
  "author": "slee",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "apollo-datasource-rest": "0.13.0",
    "apollo-server": "2.24.0",
    "apollo-server-express": "2.24.0",
    "bcrypt": "5.0.1",
    "compression": "1.7.4",
    "cors": "2.8.5",
    "dataloader": "2.0.0",
    "dotenv": "9.0.2",
    "express": "4.17.1",
    "graphql": "15.5.0",
    "graphql-iso-date": "3.6.1",
    "graphql-resolvers": "0.4.2",
    "jsonwebtoken": "8.5.1",
    "pg": "8.6.0",
    "pg-hstore": "2.3.3",
    "sequelize": "5.22.4"
  },
  "devDependencies": {
    "@babel/cli": "7.14.3",
    "@babel/core": "7.14.2",
    "@babel/node": "7.14.2",
    "@babel/preset-env": "7.14.2",
    "@babel/register": "7.13.16",
    "axios": "0.21.1",
    "chai": "5.1.2",
    "eslint": "^7.2.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-config-prettier": "4.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "3.0.1",
    "mocha": "10.7.3",
    "nodemon": "2.0.7",
    "prettier": "2.3.0",
    "prettier-eslint": "12.0.0",
    "rimraf": "2.6.3"
  }
}
