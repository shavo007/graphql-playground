{
  "name": "api",
  "version": "1.0.0",
  "description": "graphql express api",
  "main": "index.js",
  "scripts": {
    "build": "rimraf dist && babel src -d dist",
    "serve": "node dist/index.js",
    "start": "NODE_ENV=development nodemon --exec babel-node src/index.js",
    "start:prod": "NODE_ENV=production nodemon --exec babel-node src/index.js",
    "test-server": "TEST_DATABASE=mytestdatabase yarn start",
    "test": "mocha --require @babel/register 'src/**/*.spec.js'",
    "format": "prettier --write \"**/*.js\""
  },
  "author": "slee",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "apollo-datasource-rest": "0.2.1",
    "apollo-server": "2.3.1",
    "apollo-server-express": "2.3.1",
    "bcrypt": "3.0.2",
    "compression": "1.7.3",
    "cors": "2.8.5",
    "dataloader": "1.4.0",
    "dotenv": "6.1.0",
    "express": "4.16.4",
    "graphql": "14.0.2",
    "graphql-iso-date": "3.6.1",
    "graphql-resolvers": "0.2.2",
    "jsonwebtoken": "8.4.0",
    "pg": "7.7.1",
    "pg-hstore": "2.3.2",
    "sequelize": "4.41.2"
  },
  "devDependencies": {
    "@babel/cli": "7.2.0",
    "@babel/core": "7.2.2",
    "@babel/node": "7.0.0",
    "@babel/preset-env": "7.1.6",
    "@babel/register": "7.0.0",
    "axios": "0.18.0",
    "chai": "4.2.0",
    "eslint": "5.9.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-config-prettier": "3.3.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-prettier": "2.7.0",
    "mocha": "5.2.0",
    "nodemon": "1.18.7",
    "prettier": "1.14.3",
    "prettier-eslint": "8.8.2",
    "rimraf": "2.6.2"
  }
}
